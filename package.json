{
  "name": "webpack-threejs",
  "version": "1.0.1",
  "description": "A simple boilerplate to start projects with Three.js using webpack.",
  "private": true,
  "keywords": [
    "three.js",
    "webpack",
    "glsl",
    "glslify"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "webpack -p --config config/prod.js",
    "start": "webpack-dev-server --config config/dev.js",
    "deploy": "npm run build && echo \"imakeartwith.tech\" > dist/CNAME && gh-pages -d dist"
  },
  "dependencies": {
    "@brunoimbrizi/controlkit": "^0.1.99",
    "browser-detect": "^0.2.28",
    "domready": "^1.0.8",
    "glsl-noise": "0.0.0",
    "gsap": "^2.1.3",
    "stats.js": "^0.17.0",
    "three": "^0.98.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^3.2.0",
    "file-loader": "^1.1.11",
    "glslify": "^6.4.1",
    "glslify-import-loader": "^0.1.2",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
